    "description": "Updated syntax highlighting for YAGPDB Custom Commands",
    "author": {
        "name": "ranger-4297",
        "url": "https://github.com/ranger-4297"
    },
    "categories": [
        "Programming Languages"
    ],
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#EC7760",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.53.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ranger-4297/yagpdb-cc-ext"
    },
    "contributes": {
        "configuration": {
            "title": "Grammars",
            "properties": {
                "yagpdbcc.syntaxHighlighting": {
                    "description": "Specifies which language file highlight based on",
                    "type": "string",
                    "default": "yagpdbcc.tmLanguage",
                    "enum": [
                        "yagpdbcc.tmLanguage",
                        "yagpdbcc_markup.tmLanguage"
                    ]
                }
            }
        },
        "languages": [
            {
                "id": "yagpdbcc",
                "aliases": [
                    "YAGPDB CC",
                    "yag",
                    "yagpdb",
                    "yagcc",
                    "yagpdbcc",
                    "YAGPDB Custom Commands"
                ],
                "extensions": [
                    ".yag",
                    ".yagpdb",
                    ".yagcc",
                    ".yagpdbcc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "yagpdbcc",
                "scopeName": "source.yag",
                "path": "./syntaxes/yagpdbcc.tmLanguage.json"
            }
        ]
    },
